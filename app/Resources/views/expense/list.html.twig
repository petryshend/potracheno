{% extends 'base.html.twig' %}

{% block body %}
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Amount</th>
                <th>Created At</th>
            </tr>
        </thead>
        <tbody>
            {% for expense in expenses %}
                <tr>
                    <td>{{ expense.id }}</td>
                    <td>{{ expense.amount }}</td>
                    <td>{{ expense.createdAt|date }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>
{% endblock %}